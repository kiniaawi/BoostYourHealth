{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": "last 2 versions"
                },
                "modules": "commonjs",
                "loose": false
            }
        ]
    ],
    "plugins": [
        "transform-class-properties",
        "@babel/plugin-transform-modules-commonjs"
    ],
    "env": {
        "test": {
            "plugins": [
                //"transform-es2015-modules-commonjs"
            ]
        }
    }
}